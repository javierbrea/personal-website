{
  "name": "javier-brea-personal-website",
  "private": true,
  "description": "Javier Brea's personal website",
  "version": "0.1.0",
  "author": "Javier Brea <javier.brea@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/javierbrea/personal-website"
  },
  "bugs": {
    "url": "https://github.com/javierbrea/personal-website/issues"
  },
  "scripts": {
    "lint": "eslint *.js src cypress",
    "lint:staged": "lint-staged",
    "build": "gatsby build",
    "build-and-serve": "npm run build && npm run serve",
    "start": "gatsby develop -p 3000",
    "serve": "serve -l 3000",
    "stylelint": "stylelint src/**/*.scss",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "cypress:install": "cypress install",
    "cypress:verify": "cypress verify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:ci": "start-server-and-test build-and-serve http-get://localhost:3000 cypress:run",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@popperjs/core": "2.11.4",
    "bootstrap": "5.1.3",
    "clsx": "1.1.1",
    "gatsby": "4.10.3",
    "gatsby-plugin-gtag": "1.0.13",
    "gatsby-plugin-manifest": "4.10.2",
    "gatsby-plugin-module-resolver": "1.0.3",
    "gatsby-plugin-preconnect": "1.3.0",
    "gatsby-plugin-react-helmet": "5.10.0",
    "gatsby-plugin-robots-txt": "1.7.0",
    "gatsby-plugin-sass": "5.10.2",
    "gatsby-plugin-sharp": "4.10.2",
    "gatsby-plugin-sitemap": "5.10.2",
    "gatsby-source-filesystem": "4.10.1",
    "gatsby-transformer-sharp": "4.10.0",
    "jquery": "3.6.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-github-btn": "1.2.2",
    "react-helmet": "6.1.0",
    "react-images": "1.1.7",
    "react-scroll-parallax": "3.0.5",
    "react-use-scroll-position": "2.0.0",
    "typeface-heebo": "1.1.13",
    "typeface-roboto": "1.1.13"
  },
  "devDependencies": {
    "@testing-library/cypress": "8.0.2",
    "@babel/eslint-parser": "7.17.0",
    "babel-plugin-module-resolver": "4.1.0",
    "cypress": "9.5.3",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "lint-staged": "12.3.7",
    "prettier": "2.6.1",
    "sass": "1.49.9",
    "serve": "13.0.2",
    "start-server-and-test": "1.14.0",
    "stylelint": "14.6.1",
    "stylelint-config-recommended": "7.0.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-scss": "4.2.0",
    "webpack": "5.70.0"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "cypress/**/*.js": "eslint",
    "/*.js": "eslint",
    "src/**/*.scss": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
